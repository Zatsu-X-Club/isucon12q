pure_all :: $(INST_SCRIPT)/pt-align $(INST_SCRIPT)/pt-archiver $(INST_SCRIPT)/pt-config-diff $(INST_SCRIPT)/pt-deadlock-logger $(INST_SCRIPT)/pt-diskstats $(INST_SCRIPT)/pt-duplicate-key-checker $(INST_SCRIPT)/pt-fifo-split $(INST_SCRIPT)/pt-find $(INST_SCRIPT)/pt-fingerprint $(INST_SCRIPT)/pt-fk-error-logger $(INST_SCRIPT)/pt-heartbeat $(INST_SCRIPT)/pt-index-usage $(INST_SCRIPT)/pt-ioprofile $(INST_SCRIPT)/pt-kill $(INST_SCRIPT)/pt-mext $(INST_SCRIPT)/pt-mysql-summary $(INST_SCRIPT)/pt-online-schema-change $(INST_SCRIPT)/pt-pmp $(INST_SCRIPT)/pt-query-digest $(INST_SCRIPT)/pt-show-grants $(INST_SCRIPT)/pt-sift $(INST_SCRIPT)/pt-slave-delay $(INST_SCRIPT)/pt-slave-find $(INST_SCRIPT)/pt-slave-restart $(INST_SCRIPT)/pt-stalk $(INST_SCRIPT)/pt-summary $(INST_SCRIPT)/pt-table-checksum $(INST_SCRIPT)/pt-table-sync $(INST_SCRIPT)/pt-table-usage $(INST_SCRIPT)/pt-upgrade $(INST_SCRIPT)/pt-variable-advisor $(INST_SCRIPT)/pt-visual-explain
	$(NOECHO) $(NOOP)

realclean ::
	$(RM_F) \
	  $(INST_SCRIPT)/pt-align $(INST_SCRIPT)/pt-archiver \
	  $(INST_SCRIPT)/pt-config-diff $(INST_SCRIPT)/pt-deadlock-logger \
	  $(INST_SCRIPT)/pt-diskstats $(INST_SCRIPT)/pt-duplicate-key-checker \
	  $(INST_SCRIPT)/pt-fifo-split $(INST_SCRIPT)/pt-find \
	  $(INST_SCRIPT)/pt-fingerprint $(INST_SCRIPT)/pt-fk-error-logger \
	  $(INST_SCRIPT)/pt-heartbeat $(INST_SCRIPT)/pt-index-usage \
	  $(INST_SCRIPT)/pt-ioprofile $(INST_SCRIPT)/pt-kill \
	  $(INST_SCRIPT)/pt-mext $(INST_SCRIPT)/pt-mysql-summary \
	  $(INST_SCRIPT)/pt-online-schema-change $(INST_SCRIPT)/pt-pmp \
	  $(INST_SCRIPT)/pt-query-digest $(INST_SCRIPT)/pt-show-grants \
	  $(INST_SCRIPT)/pt-sift $(INST_SCRIPT)/pt-slave-delay \
	  $(INST_SCRIPT)/pt-slave-find $(INST_SCRIPT)/pt-slave-restart \
	  $(INST_SCRIPT)/pt-stalk $(INST_SCRIPT)/pt-summary \
	  $(INST_SCRIPT)/pt-table-checksum $(INST_SCRIPT)/pt-table-sync \
	  $(INST_SCRIPT)/pt-table-usage $(INST_SCRIPT)/pt-upgrade \
	  $(INST_SCRIPT)/pt-variable-advisor $(INST_SCRIPT)/pt-visual-explain 

$(INST_SCRIPT)/pt-align : bin/pt-align $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)/pt-align
	$(CP) bin/pt-align $(INST_SCRIPT)/pt-align
	$(FIXIN) $(INST_SCRIPT)/pt-align
	-$(NOECHO) $(CHMOD) $(PERM_RWX) $(INST_SCRIPT)/pt-align

